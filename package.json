    "test:snapshot": "vitest -u --config test/vitest.config.mjs"
  },
  "dependencies": {
    "array-tree-filter": "^3.0.3",
    "es-toolkit": "^1.39.8"
  },
  "devDependencies": {
    "@bring-it/npm": "^0.8.3",
    "@nice-move/all-in-base": "^0.6.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "garou": "^0.9.11",
    "prettier": "~3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": "^22.0.0"
